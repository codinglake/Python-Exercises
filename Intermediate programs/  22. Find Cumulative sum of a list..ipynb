    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Cumulative list: [10, 30, 60, 100, 150]\n"
     ]
    }
   ],
   "source": [
    "lst = [10, 20, 30, 40, 50]\n",
    "print(\"Cumulative list:\",sol(lst))"
   ]
  }
