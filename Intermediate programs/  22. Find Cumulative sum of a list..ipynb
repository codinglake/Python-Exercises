   "source": [
    "def sol(lst):\n",
    "    clst =[]\n",
    "    sum =0\n",
    "    for i in lst:\n",
    "        sum = sum + i\n",
    "        # get the cumulative sum in clst\n",
    "        clst.append(sum)\n",
    "    return clst"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
